{
  "name": "backyourstack",
  "version": "1.1.0",
  "description": "BackYourStack analyzes repositories to recommend Open Source projects to support.",
  "repository": {
    "type": "git",
    "url": "https://github.com/opencollective/backyourstack.git"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node -- src/server/index.js",
    "build": "npm run build:server && npm run build:next",
    "build:server": "babel --copy-files ./src --out-dir ./dist",
    "build:next": "next build dist",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "prettier": "prettier src/**/*.js test/**/*.js --write",
    "test": "NODE_ENV=test jest",
    "update-projects": "babel-node src/scripts/update-projects",
    "update-collectives": "babel-node src/scripts/update-collectives",
    "detect-packages": "./src/scripts/detect-all-packages.sh",
    "clean": "rm -rf dist src/.next"
  },
  "author": {
    "name": "Open Collective, Inc.",
    "email": "team@opencollective.com"
  },
  "contributors": ["Fran√ßois Hodierne <francois@opencollective.com>"],
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^15.12.0",
    "@zeit/next-css": "^1.0.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "cross-fetch": "^2.2.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "fs-extra": "^7.0.0",
    "gemfile": "^1.1.0",
    "graphql-request": "^1.8.2",
    "lodash": "^4.17.11",
    "lru-cache": "^4.1.3",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "multer": "^1.3.1",
    "next": "7.0.1",
    "next-routes": "^1.4.2",
    "nprogress": "^0.2.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-dropzone": "^6.0.0",
    "serve-favicon": "^2.5.0",
    "toml": "^2.3.3",
    "winston": "^3.1.0",
    "xmldoc": "^1.1.2"
  },
  "engines": {
    "node": "10.x.x"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/node": "7.0.0",
    "@mdx-js/loader": "^0.15.2",
    "@mdx-js/mdx": "^0.15.2",
    "babel-eslint": "^10.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-opencollective": "^1.5.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "svg-react-loader": "^0.4.5",
    "webpack": "4.20.2",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "nodemonConfig": {
    "watch": ["src/lib", "src/server", "src/routes.js"]
  },
  "greenkeeper": {
    "ignore": ["@babel/cli", "@babel/node", "webpack"]
  }
}
