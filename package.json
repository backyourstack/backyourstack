{
  "name": "backyourstack",
  "version": "1.1.0",
  "description": "BackYourStack analyzes repositories to recommend Open Source projects to support.",
  "repository": {
    "type": "git",
    "url": "https://github.com/backyourstack/backyourstack.git"
  },
  "scripts": {
    "clean": "npm run build:clean && npm run test:clean",
    "deploy:production": "git push production main",
    "deploy:staging": "git push -f staging main",
    "dev": "nodemon --exec babel-node -- src/index.js",
    "build": "npm run build:clean && npm run build:server && npm run build:next",
    "build:clean": "rimraf dist .next",
    "build:server": "babel --copy-files ./src --out-dir ./dist",
    "build:next": "next build",
    "start": "NODE_ENV=production node dist/index.js",
    "start:next": "NODE_ENV=production next start",
    "lint": "eslint \"@(components|lib|pages|scripts|src|test)/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "prettier": "prettier \"*.@(js|json|md)\" \"@(components|data|lib|pages|scripts|src|test)/**/*.@(js|json|md)\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --list-different",
    "test": "NODE_ENV=test jest",
    "test:update": "NODE_ENV=test jest --updateSnapshot",
    "test:clean": "rimraf coverage",
    "coverage": "codecov",
    "update-collectives": "babel-node ./scripts/update-collectives",
    "detect-packages": "./scripts/detect-all-packages.sh",
    "sync-projects-with-opencollective": "babel-node ./scripts/sync-projects-with-opencollective"
  },
  "author": {
    "name": "Open Collective, Inc.",
    "email": "team@opencollective.com"
  },
  "contributors": [
    "Fran√ßois Hodierne <francois@opencollective.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.12.10",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@octokit/rest": "^18.12.0",
    "aws-sdk": "^2.1133.0",
    "bluebird": "^3.7.2",
    "classnames": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "gemfile": "^1.1.0",
    "graphql-request": "^2.0.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "md5": "^2.3.0",
    "minimatch": "^3.0.4",
    "moment": "^2.29.1",
    "multer": "^1.4.3",
    "next": "11.1.1",
    "next-routes": "^1.4.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.7.0",
    "nprogress": "^0.2.0",
    "passport": "^0.5.0",
    "passport-github": "^1.1.0",
    "prop-types": "^15.7.2",
    "pure-react-carousel": "^1.27.6",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^7.0.1",
    "react-icons": "^4.3.1",
    "react-modal": "^3.14.3",
    "react-number-format": "^4.8.0",
    "serve-favicon": "^2.5.0",
    "styled-jsx": "4.0.1",
    "toml": "^3.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "xmldoc": "^1.1.2"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/node": "7.15.4",
    "@mdx-js/loader": "^1.6.22",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-opencollective": "^3.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.26.1",
    "file-loader": "^6.2.0",
    "husky": "^4.3.8",
    "jest": "^27.3.1",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "svg-react-loader": "^0.4.6",
    "thanks": "^2.3.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "nodemonConfig": {
    "watch": [
      "src/server",
      "src/routes.js",
      "src/lib"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "jsdom"
  }
}
